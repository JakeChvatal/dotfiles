"poweroff"
  Mod4 + Escape
"reboot"
  Mod4 + Shift + Escape
#"/usr/bin/spotify --force-device-scale-factor=2"
#  Mod4 + Tab
"thunar"
  Mod4 + BackSpace
"firefox"
  Mod4 + backslash
"xkill"
  End
# "emacs"
#   Mod4 + Delete 

# VOLUME KEYS
"pulseaudio-ctl up 5"
  XF86AudioRaiseVolume
"pulseaudio-ctl down 5"
  XF86AudioLowerVolume
"pulseaudio-ctl mute"
  XF86AudioMute

# BRIGHTNESS KEYS
"light -A 10"
  XF86MonBrightnessUp
"light -U 10"
  XF86MonBrightnessDown
"light -A 5"
  Shift+XF86MonBrightnessUp
"light -U 5"
  Shift+XF86MonBrightnessDown
"light -A 1"
  Mod4+Shift+XF86MonBrightnessUp
"light -U 1"
  Mod4+Shift+XF86MonBrightnessDown

# MUSIC
"playerctl play-pause"
  XF86AudioPlay
"playerctl play-pause"
  Mod4 + Shift + slash

"playerctl previous"
  XF86AudioPrev
"playerctl previous"
  Mod4 + Shift + comma

"playerctl next"
  XF86AudioNext

